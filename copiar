# Archivo de entrada
archivo="datos.txt"

# Leer línea por línea del archivo
while IFS='|' read -r campo1 campo2 campo3 || [[ -n "$campo1" ]]; do
    # Mostrar o procesar cada campo según sea necesario
    echo "Campo 1: $campo1"
    echo "Campo 2: $campo2"
    echo "Campo 3: $campo3"
done < "$archivo"
